<div id="Complete a Meal" class="module">
  <h2>Complete a Meal</h2>
  <table class="w3-table w3-card">
    <tr>
      <th>buy:</th>
      <th>to make:</th>
    </tr>
    {% for row in data.CAM %}
    <tr>
      <td><a href="{{ row.ingredient_link }}">{{ row.ingredient_name }} {{ row.ingredient_amount_max }} {{ row.ingredient_measure }}</a></td>
      <td><span class="w3-badge w3-yellow w3-margin-right">{{ row.amount }}</span><a href="{{ row.meal_link }}">{{ row.meal_name }}</a></td>
    </tr>
    {% endfor %}
  </table>
</div>
